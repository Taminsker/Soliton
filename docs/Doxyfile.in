OUTPUT_DIRECTORY        = @CMAKE_SOURCE_DIR@/docs/doc_doxygen
INPUT                   = @CMAKE_SOURCE_DIR@/soliton_src
EXCLUDE_PATTERNS        = *.txt
PROJECT_NAME            = "Soliton"
GENERATE_MAN            = YES
QUIET                   = YES
WARNINGS                = YES
OUTPUT_LANGUAGE         = English
RECURSIVE               = YES
GENERATE_XML            = NO
GENERATE_LATEX 		= YES
COMPACT_LATEX           = YES
DOXYFILE_ENCODING       = UTF-8
EXTRACT_ALL             = YES
EXTRACT_PRIVATE         = YES
EXTRACT_STATIC          = YES
ALWAYS_DETAILED_SEC     = NO
SHOW_FILES 		= YES
#DOT_PATH               = @CMAKE_SOURCE_DIR@/docs/doc_doxygen/dot
DOT_PATH		= "@DOXYGEN_DOT_PATH@"
HAVE_DOT                = YES
COLLABORATION_GRAPH     = YES
LATEX_SOURCE_CODE       = YES
SOURCE_BROWSER          = YES
INLINE_SOURCES          = YES
HIDE_UNDOC_MEMBERS      = NO
HIDE_UNDOC_CLASSES      = NO
HIDE_FRIEND_COMPOUNDS   = NO
HIDE_IN_BODY_DOCS       = NO
INLINE_INHERITED_MEMB   = YES
WARN_IF_UNDOCUMENTED    = YES
ENABLE_PREPROCESSING    = YES
MACRO_EXPANSION         = YES
EXPAND_ONLY_PREDEF      = NO
SHOW_NAMESPACES         = YES
SHOW_INCLUDE_FILES      = YES
HIDE_SCOPE_NAMES        = NO
SHOW_USED_FILES         = YES
TYPEDEF_HIDES_STRUCT    = NO
REPEAT_BRIEF            = YES
SEPARATE_MEMBER_PAGES   = YES
ALIASES                += today="@TODAY@"

DISABLE_INDEX           = NO
GENERATE_TREEVIEW       = YES
FULL_PATH_NAMES         = NO

DOT_GRAPH_MAX_NODES     = 20
MAX_DOT_GRAPH_DEPTH     = 1
COLLABORATION_GRAPH     = YES
